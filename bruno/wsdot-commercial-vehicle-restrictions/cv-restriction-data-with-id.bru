get {
  url: "{{wsdot_commercial_vehicle_restrictions_base_url}}/getCVRByRestrictionIDAsJson"
  auth {
    type: query
    params {
      AccessCode: "{{api_access_token}}"
      RestrictionID: "{{cv_restriction_id}}"
    }
  }
}

description: "Returns commercial vehicle restriction data for the specified restriction ID."

