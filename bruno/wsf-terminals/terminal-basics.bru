get {
  url: "{{wsf_terminals_base_url}}/terminalBasics"
  auth {
    type: query
    params {
      api_access_code: "{{api_access_token}}"
    }
 }
}

description: "Returns a list of TerminalBasic data for all terminals."

---

get {
  url: "{{wsf_terminals_base_url}}/terminalBasics/{{terminal_id}}"
  auth {
    type: query
    params {
      api_access_code: "{{api_access_token}}"
    }
 }
}

description: "Returns TerminalBasic data for the terminal with the given identifier."

---