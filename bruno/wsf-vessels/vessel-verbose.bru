get {
  url: "{{wsf_vessels_base_url}}/vesselVerbose"
  auth {
    type: query
    params {
      api_access_code: "{{api_access_token}}"
    }
  }
}

description: "Returns a list of VesselVerbose data for all vesselVerboses."

