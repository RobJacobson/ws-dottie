{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.expo": true
  },
  "hide-files.files": [],
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
